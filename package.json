{
  "name": "codingame-js-starter",
  "version": "1.0.0",
  "description": "A starter template for CodinGame JavaScript challenge",
  "author": "Volcomix",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "rollup --config --watch",
    "build": "rollup --config",
    "test": "jest --watch",
    "test:ci": "jest",
    "profile": "node --prof dist/bundle.js",
    "0x": "0x -D prof -o dist/bundle.js",
    "typecheck": "tsc --noEmit",
    "fetch:puzzle": "ts-node scripts/fetchPuzzle.ts"
  },
  "devDependencies": {
    "0x": "^4.10.2",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.4",
    "axios": "^0.21.1",
    "babel-jest": "^27.0.5",
    "jest": "^27.0.6",
    "rollup": "^2.52.2",
    "ts-node": "^10.0.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4"
  }
}
