{
  "name": "codingame-js-starter",
  "version": "1.0.0",
  "description": "A starter template for CodinGame JavaScript challenge",
  "author": "Volcomix",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "rollup --config --watch",
    "build": "rollup --config",
    "test": "jest --watch",
    "test:ci": "jest",
    "profile": "node --prof dist/bundle.js",
    "0x": "0x -D prof -o dist/bundle.js",
    "typecheck": "tsc --noEmit",
    "fetch:puzzle": "ts-node scripts/fetchPuzzle.ts"
  },
  "devDependencies": {
    "0x": "^5.1.2",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.21",
    "axios": "^0.26.0",
    "babel-jest": "^27.5.1",
    "jest": "^27.5.1",
    "rollup": "^2.67.3",
    "ts-node": "^10.5.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  }
}
